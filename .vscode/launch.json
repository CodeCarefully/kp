  "version": "0.2.0",

  "configurations": [
    {
      "name": "Launch With Test DB",
      "type": "go",
      "request": "launch",
      "mode": "exec",
      "program": "./kp",
      "env": {
        "KP_DATABASE": "./local_artifacts/test.kdb",
        "KP_PASSWORD": "password"
      },
      "console": "integratedTerminal",
      "preLaunchTask": "prep",
      "buildFlags": "-mod=readonly"
    }
  ]
}